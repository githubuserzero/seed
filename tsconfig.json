{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "moduleResolution": "node",
    "jsx": "react",
    "skipLibCheck": true,
    "sourceMap": true,
    "experimentalDecorators": true,
    "allowJs": false,
    "downlevelIteration": true,
    "lib": [
      "dom",
      "es2015",
      "es2016"
    ],
    "baseUrl": "src",
    "paths": {
      "actions": [
        "shared/actions"
      ],
      "components": [
        "shared/components"
      ],
      "data": [
        "shared/data"
      ],
      "frame": [
        "shared/frame"
      ],
      "services": [
        "shared/services"
      ],
      "stores": [
        "shared/stores"
      ],
      "styles": [
        "shared/styles"
      ],
      "utils": [
        "shared/utils"
      ],
      "messages/*": [
        "shared/messages/*"
      ],
      "contents/*": [
        "shared/contents/*"
      ],
      "templates": [
        "shared/templates"
      ],
      "open-react-modal": [
        "shared/open-react-modal"
      ]
    }
  }
}