{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "moduleResolution": "node",
    "jsx": "react",
    "skipLibCheck": true,
    "sourceMap": true,
    "experimentalDecorators": true,
    "allowJs": false,
    "downlevelIteration": true,
    "lib": [
      "dom",
      "es2015",
      "es2016"
    ],
    "baseUrl": "src",
    "paths": {
      "_messages/*": [
        "_messages/*"
      ],
      "common": [
        "common"
      ],
      "common/*": [
        "common/*"
      ],
      "app": [
        "app"
      ],
      "app/*": [
        "app/*"
      ],
      "showcase": [
        "showcase"
      ],
      "showcase/*": [
        "showcase/*"
      ],
      "open-react-modal": [
        "_library/open-react-modal"
      ]
    }
  }
}