{
  "name": "seed",
  "version": "2017.1.0",
  "description": "{project-description}",
  "scripts": {
    "build:prod": "npm rebuild && webpack --env.action=build.production",
    "build:dev": "webpack --env.action=build.dev",
    "build:dll": "webpack --env.action=build.dll",
    "serve": "npm rebuild && node browser-sync.js",
    "build": "npm run build:prod",
    "start": "npm run serve",
    "test": "jest",
    "coverage": "jest --coverage",
    "ghpages": "git subtree push --prefix dist origin gh-pages",
    "lint": "tslint 'src/**/!(*.spec|*.test).ts?(x)'",
    "build:libs": "sh build-libs.sh",
    "publish:libs": "sh publish-libs.sh",
    "libs": "npm run build:libs && npm run publish:libs"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iamssen/seed.git"
  },
  "upstream": {
    "git": "https://github.com/iamssen/seed.git"
  },
  "bugs": {
    "url": "https://github.com/iamssen/seed/issues"
  },
  "homepage": "https://github.com/iamssen/seed",
  "dependencies": {
    "core-js": "^2.4.1",
    "d3-array": "^1.2.0",
    "i18next": "^8.4.3",
    "i18next-browser-languagedetector": "^2.0.0",
    "i18next-xhr-backend": "^1.4.1",
    "ie-shim": "^0.1.0",
    "immutable": "4.0.0-rc.2",
    "mobx": "^3.2.2",
    "mobx-dispatcher": "^0.1.3",
    "mobx-react": "^4.2.2",
    "moment": "^2.18.1",
    "numeral": "^2.0.6",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-i18next": "^5.1.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.0",
    "styled-components": "^2.1.1",
    "url-search-params-polyfill": "^2.0.0"
  },
  "devDependencies": {
    "@types/d3-array": "^1.2.1",
    "@types/i18next": "^8.4.1",
    "@types/jest": "^20.0.8",
    "@types/numeral": "^0.0.22",
    "@types/prop-types": "^15.5.1",
    "@types/react": "^16.0.5",
    "@types/react-dom": "^15.5.4",
    "@types/react-i18next": "^4.6.0",
    "@types/react-router": "^4.0.15",
    "@types/react-router-dom": "^4.0.4",
    "@types/webpack-env": "^1.13.0",
    "autoprefixer": "^7.1.2",
    "awesome-typescript-loader": "^3.2.3",
    "browser-sync": "^2.18.13",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.5",
    "cssnano": "^3.10.0",
    "cssnext": "^1.8.4",
    "enzyme": "^2.9.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "glob": "^7.1.2",
    "jest": "^20.0.3",
    "jsdom": "^11.2.0",
    "json-loader": "^0.5.7",
    "markdown-loader": "^2.0.1",
    "node-sass": "^4.5.3",
    "package-json": "^4.0.1",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "pre-commit": "^1.2.2",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.5.1",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "sass-variable-loader": "^0.1.2",
    "semver": "^5.4.1",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.0",
    "ts-jest": "^20.0.8",
    "tslint": "^5.6.0",
    "typescript": "^2.4.2",
    "webpack": "^3.5.5",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-merge": "^4.1.0",
    "webpack-node-externals": "^1.6.0"
  },
  "pre-commit": [
    "lint"
  ],
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/**/*.ts?(x)",
      "**/?(*.)(spec|test).ts?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src/shared"
    ],
    "coverageDirectory": "coverage"
  }
}
