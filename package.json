{
  "name": "seed",
  "version": "2019.4.0",
  "private": true,
  "scripts": {
    "app:build": "zeroconfig-webapp-scripts build app --size-report false",
    "app:dev:build": "npm run app:build --- --mode development",
    "app:start": "zeroconfig-webapp-scripts start app",
    "package:build": "zeroconfig-package-scripts build",
    "package:publish": "zeroconfig-package-scripts publish",
    "storybook:start": "start-storybook",
    "storybook:build": "build-storybook",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "build": "npm run app:build",
    "start": "npm run app:start",
    "test": "jest --colors",
    "test:e2e:build": "sh e2e/build.sh",
    "coverage": "jest --colors --coverage && open coverage/index.html",
    "lint": "tslint \"src/**/*.{ts,tsx}\"",
    "git:publish:browser": "git subtree push --prefix dist/browser origin dist/app/browser",
    "git:publish:server": "git subtree push --prefix dist/server origin dist/app/server",
    "git:publish": "npm run git:publish:web && npm run git:publish:server"
  },
  "pre-commit": [
    "test",
    "lint"
  ],
  "jest": {
    "preset": "react-zeroconfig",
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coverageReporters": [
      "html"
    ]
  },
  "dependencies": {
    "@loadable/component": "^5.10.1",
    "@loadable/server": "^5.9.0",
    "d3-array": "^2.2.0",
    "ejs": "^2.6.1",
    "history": "^4.9.0",
    "immer": "^3.1.2",
    "js-cookie": "^2.2.0",
    "koa": "^2.7.0",
    "koa-router": "^7.4.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.25",
    "node-fetch": "^2.6.0",
    "numeral": "^2.0.6",
    "prismjs": "^1.15.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-app-polyfill": "^1.0.1",
    "react-devdoc": "^0.1.4",
    "react-dom": "^16.8.4",
    "react-intl": "^3.0.0-beta-7",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-router-store": "^0.3.1",
    "regenerator-runtime": "^0.13.2",
    "url-search-params-polyfill": "^6.0.0",
    "use-locale": "^0.2.0",
    "use-react-intl": "^0.1.2",
    "use-react-router": "^1.0.7",
    "use-timezone": "^0.1.3"
  },
  "devDependencies": {
    "@storybook/react": "^5.1.3",
    "@types/compression": "0.0.36",
    "@types/core-js": "^2.5.0",
    "@types/d3-array": "^2.0.0",
    "@types/ejs": "^2.6.3",
    "@types/history": "^4.7.2",
    "@types/jest": "^24.0.13",
    "@types/js-cookie": "^2.2.1",
    "@types/koa": "^2.0.48",
    "@types/koa-router": "^7.0.40",
    "@types/loadable__component": "^5.9.0",
    "@types/loadable__server": "^5.9.0",
    "@types/loader-utils": "^1.1.3",
    "@types/mkdirp": "^0.5.2",
    "@types/moment-timezone": "^0.5.12",
    "@types/node-fetch": "^2.3.5",
    "@types/numeral": "0.0.25",
    "@types/prismjs": "^1.9.1",
    "@types/prop-types": "^15.7.1",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/react-intl": "^2.3.18",
    "@types/react-loadable": "^5.5.1",
    "@types/react-router": "^5.0.0",
    "@types/react-router-dom": "^4.3.3",
    "@types/storybook__react": "^4.0.2",
    "@types/webpack-env": "^1.13.9",
    "cypress": "^3.3.1",
    "jest": "^24.8.0",
    "mkdirp": "^0.5.1",
    "multiplerun": "^0.2.2",
    "pre-commit": "^1.2.2",
    "react-zeroconfig": "^3.3.1",
    "surge": "^0.21.3",
    "tslint": "^5.17.0",
    "tslint-ssen-rules": "^1.3.0",
    "typescript": "^3.5.1"
  }
}
